whatis build >/dev/null >[2=1]
if(! ~ $status '') {
	fn build {
		if(! cd $wrksrc) {
			echo Couldn''t find wrksrc $builddir/$wrksrc
			echo 'wrksrc should probably be one of'
			ls
			exit no_wrksrc
		}
		mk all
	}
}

whatis install >/dev/null >[2=1]
if(! ~ $status '') {
	fn install {
		mk install
	}
}
